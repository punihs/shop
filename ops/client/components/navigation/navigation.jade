//- list
ul.nav.navbar-nav.navbar-left
  li.hidden-folded.padder.m-t.m-b-sm.text-muted.text-xs Navigation
  li.nav-menu-item(ui-sref-active='active'): a.hv(ui-sref='feeds.index({bucket: "PACKAGE"})')
    i.fa.fa-dashboard.text-white
    span DASHBOARD
  li.nav-menu-item(ui-sref-active='active' uib-dropdown)
    a.hv(uib-dropdown-toggle ui-sref='orders.index({bucket: "All"})')
      i.fa.fa-list-alt.text-white
      span ORDERS
  li.nav-menu-item(ui-sref-active='active'): a.hv(ui-sref='packages.index({ bucket: "All"})')
    i.fa.fa-users.text-white
    span PACKAGES
  li.nav-menu-item(ui-sref-active='active' uib-dropdown)
    a.hv(uib-dropdown-toggle ui-sref='shipments.index({ bucket: "All"})')
      i.fa.fa-list-alt.text-white
      span SHIPMENTS
  li.nav-menu-item(ui-sref-active='active' uib-dropdown)
    a.hv(uib-dropdown-toggle ui-sref='customers.list({status: "All"})')
      i.fa.fa-list-alt.text-white
      span CUSTOMERS
  li.nav-menu-item(ui-sref-active='active' uib-dropdown, ng-controller = "CampaignController as $ctrl")
    a.hv(uib-dropdown-toggle, ng-click= '$ctrl.accessCampaign()')
      i.fa.fa-list-alt.text-white
      span CAMPAIGNS
  li.nav-menu-item(ui-sref-active='active' uib-dropdown ng-controller='AccessCourierController as $ctrl')
    a.hv(ng-click="$ctrl.accessCourier()")
      i.fa.fa-list-alt.text-white
      span ACCESS COURIER



