{
  "name": "lambda",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "sesMetric": {
    "projectId": 1,
    "directories": [
      "server/api/package/emails"
    ]
  },
  "scripts": {
    "start": "nodemon --ignore node_modules/ -e js server/app",
    "lint": "eslint --fix --ext .js --ignore-path .gitignore ./server",
    "test": "NODE_ENV=test nyc --reporter=text mocha --opts .mocharc server/api/**/*.spec.js --exit | grep -v '100'",
    "kill": "kill -9 $(lsof -t -i:5004);",
    "event:fire": "node ../sesmetrics-sdk events",
    "email-build": "node ../sesmetrics-sdk build",
    "email-update-send": "nodemon  ../sesmetrics-sdk update-send",
    "email-deploy": "DEBUG=q-* node ../sesmetrics-sdk deploy",
    "validate": "npm ls"
  },
  "author": "Vikas Kumar <vikas@shoppre.com>",
  "pre-commit": [
    "lint"
  ],
  "license": "ISC",
  "dependencies": {
    "addressparser": "^1.0.1",
    "aws-sdk": "^2.229.1",
    "bluebird": "^3.3.5",
    "body-parser": "^1.18.3",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "fastq": "^1.6.0",
    "handlebars": "^4.0.12",
    "lodash": "^4.17.11",
    "mailcomposer": "^4.0.2",
    "minio": "^5.0.2",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "onesignal-node": "^1.1.2",
    "plivo": "^0.4.0",
    "request-promise": "^4.2.2",
    "uuid": "^3.3.2",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2",
    "winston-raven-sentry": "^1.0.1"
  },
  "devDependencies": {
    "artillery": "^1.6.0-21",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "http-server": "^0.11.1",
    "mocha": "^5.1.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.3",
    "nyc": "^11.8.0",
    "precommit-hook": "^3.0.0",
    "semantic-release": "^15.7.0",
    "sinon": "^6.1.3",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": "^8.11.0",
    "npm": "^5.6.0"
  },
  "nodemonConfig": {
    "ignore": [
      "docs/*"
    ],
    "delay": "2500"
  }
}
